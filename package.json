  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate:embeddings && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "build:docker": "docker build -f Dockerfile.frontend -t mlai-frontend .",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:comprehensive": "node scripts/run-comprehensive-tests.js",
    "test:unit": "vitest run src/utils/__tests__",
    "test:integration": "vitest run src/components/__tests__",
    "test:performance": "vitest run src/test/performance.test.ts",
    "test:accuracy": "vitest run src/test/accuracy.test.ts",
    "test:browser": "npm run test:compatibility",
    "test:coverage": "vitest run --coverage",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist",
    "test:compatibility": "node scripts/test-browser-compatibility.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:bundle:compare": "node scripts/analyze-bundle.js --compare",
    
    
    "dev:backend": "cd backend-fastapi && python3 main.py",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "start:static-backend": "cd backend && npm run start:static",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:prod": "docker-compose --profile production up -d",
    "demo:tokenizer": "node scripts/chatbot-demo.js",
    "generate:embeddings": "node scripts/generate-embeddings.js",
    "prepare": "husky"
